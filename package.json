{
  "name": "testing-agents",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"./scripts/start-db.sh\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:db": "./scripts/start-db.sh",
    "dev:frontend": "vite",
    "dev:backend": "node backend/server.js",
    "build": "vite build",
    "preview": "vite preview",
    "db:start": "echo 'Daciapop12345' | sudo -S docker-compose up -d",
    "db:stop": "echo 'Daciapop12345' | sudo -S docker-compose down"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "concurrently": "^9.2.1",
    "svelte": "^5.39.6",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "dependencies": {
    "@langchain/openai": "^1.0.0",
    "@qdrant/js-client-rest": "^1.15.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "langchain": "^1.0.1",
    "marked": "^16.4.1",
    "openai": "^6.7.0",
    "ws": "^8.18.3"
  }
}
